        },
        "getExpensesId":{
            "method":"GET",
            "resource":"/expenses/:id",
            "uriParameters":{
                "id":"<PARAMETER_VALUE>"
            }
        },
        "getExpenses":{
            "method":"GET",
            "resource":"/expenses"
